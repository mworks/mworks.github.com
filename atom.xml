---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en-us">
  
  <title type="text">The MWorks Project</title>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://mworks-project.org/</id>
  <link rel="alternate" type="text/html" href="http://mworks-project.org/" />
  <link rel="self" type="application/atom+xml" href="http://mworks-project.org/atom.xml" />
  {% for post in site.posts %}
  <entry>
    <title type="text">{{ post.title }}</title>
    <link rel="alternate" type="text/html"  href="http://mworks-project.org{{ post.url }}" />
    <id>http://mworks-project.org{{ post.id }}</id>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
